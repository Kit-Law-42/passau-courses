# 1st solution: Recursion

class Solution:
    def uniquePaths(self, m: int, n: int) -> int:
        
        
        def tile(x, y):
            if (x==m-1 and y==n-1):
                return 0
            if (x==m-1):
                return tile(x, y+1)
            if (y==n-1):
                return tile(x+1, y)
            return 1+tile(x+1, y)+tile(x, y+1)
        
        return 1+tile(0,0)
        
        
        #2nd solution
        
